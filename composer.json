{
    "name": "voerro/file-uploader",
    "description": "A small helper class for easier file and image uploading in Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Zavyalov",
            "email": "alex@voerro.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "intervention/image": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Voerro\\FileUploader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Voerro\\FileUploader\\Test\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "orchestra/testbench": "^3.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Voerro\\FileUploader\\FileUploaderServiceProvider"
            ],
            "aliases": {
                "FileUploader": "Voerro\\FileUploader\\FileUploaderFacade"
            }
        }
    }
}
